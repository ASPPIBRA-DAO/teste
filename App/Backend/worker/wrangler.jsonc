{
  // üè∑Ô∏è IDENTIFICA√á√ÉO DO PROJETO
  "name": "governance-system",
  "main": "src/index.ts",
  "compatibility_date": "2025-01-01",
  // ID da sua conta Cloudflare
  "account_id": "5d91807e648c183cb7833caa06dbcbdb",
  
  // ‚úÖ DOM√çNIO PERSONALIZADO
  // Configura automaticamente o DNS e SSL para api.asppibra.com
  "routes": [
    {
      "pattern": "api.asppibra.com",
      "custom_domain": true
    }
  ],

  // ‚öôÔ∏è COMPATIBILIDADE E FLAGS
  // "nodejs_compat" √© essencial para bibliotecas de criptografia (bcrypt, jwt)
  "compatibility_flags": [
    "nodejs_compat"
  ],

  // üëÅÔ∏è OBSERVABILIDADE
  // Permite ver logs em tempo real no painel da Cloudflare
  "observability": {
    "enabled": true
  },

  // üü¢ ADICIONE ESTA SE√á√ÉO AQUI (Vari√°veis N√£o Confidenciais)
  // Isto impede que o Wrangler as remova.
  "vars": {
    "CLOUDFLARE_ACCOUNT_ID": "5d91807e648c183cb7833caa06dbcbdb",
    "CLOUDFLARE_ZONE_ID": "60681ad827e114d9e51add1f079dd5d2"
    // N√£o inclua o API Token neste arquivo!
  },

  // üì¶ BANCO DE DADOS (D1)
  "d1_databases": [
    {
      "binding": "DB", // Nome usado no c√≥digo (env.DB)
      "database_name": "governance-system-db",
      "database_id": "fbdff5ac-2fcc-4182-9cbf-be6c1d08e287",
      "migrations_dir": "./migrations"
    }
  ],

  // üóÇÔ∏è ARMAZENAMENTO (R2)
  "r2_buckets": [
    {
      "binding": "ASSETS", // Nome usado no c√≥digo (env.ASSETS)
      "bucket_name": "governance-system-assets",
      "preview_bucket_name": "governance-system-assets" // Bucket usado em dev (pode ser o mesmo ou outro)
    }
  ]
}